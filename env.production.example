# AcroEduvos Production Environment Variables
NODE_ENV=production
PORT=3000

# Judge0 API Configuration for Real Code Execution
JUDGE0_API_URL=https://judge0-ce.p.rapidapi.com
JUDGE0_API_KEY=YOUR_PRODUCTION_JUDGE0_API_KEY

# OpenRouter API for AI Features
OPENROUTER_API_KEY=YOUR_OPENROUTER_API_KEY
OPENROUTER_APP_NAME=AcroEduvos
OPENROUTER_APP_URL=https://acroeduvos.in

# Database Configuration (if needed)
DATABASE_URL=your_production_database_url

# Security Configuration
NEXTAUTH_SECRET=your_nextauth_secret
NEXTAUTH_URL=https://acroeduvos.in

# Analytics and Monitoring
ANALYTICS_ID=your_analytics_id
SENTRY_DSN=your_sentry_dsn

# Feature Flags
ENABLE_REAL_TIME_COMPILATION=true
ENABLE_AI_FEATURES=true
ENABLE_ANALYTICS=true
ENABLE_ERROR_REPORTING=true
